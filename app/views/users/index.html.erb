<h1>Hello World!</h1>

<h2>Photos:</h2>

<% @photos.each do |photo| %>
<div class="margin-high">
  <%= link_to image_tag(photo.post.url(:small)), photo_path(photo.id), class: "margin-high saturate gallery" %></div><br />
<% end %>

<%= form_for @photo, url: photos_path, html: { :multipart => true }, class: "form-group"  do |f| %>
  <%= f.label :post, "Post" %>
  <%= f.file_field :post %>
  <%= f.submit "Add Post", class: "btn btn-success margin-high" %>
<% end %>
